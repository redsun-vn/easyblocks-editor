{
  "name": "@redsun-vn/easyblocks-editor",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/",
    "dev": "concurrently -n bundle,emit-declarations 'npm run bundle:watch' 'npm run emit-declarations:watch'",
    "build": "npm run bundle && npm run emit-declarations",
    "bundle": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "bundle:watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch",
    "emit-declarations": "tsc",
    "emit-declarations:watch": "tsc --watch --preserveWatchOutput",
    "test": "jest",
    "test:debug": "NODE_OPTIONS='--inspect-brk' jest --runInBand",
    "check": "tsc && eslint --quiet src/"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@emotion/is-prop-valid": "^1.2.1",
    "@popperjs/core": "^2.11.5",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@react-aria/tooltip": "^3.1.8",
    "@redsun-vn/easyblocks-core": "https://github.com/redsun-vn/easyblocks-core#dev",
    "@redsun-vn/easyblocks-design-system": "file:../easyblocks-design-system",
    "final-form": "^4.20.4",
    "final-form-arrays": "^3.0.1",
    "lodash.throttle": "^4.1.1",
    "react-final-form": "^6.5.7",
    "react-modal": "^3.12.1",
    "react-popper": "^2.3.0",
    "react-remove-scroll": "^2.6.3",
    "styled-components": "^6.0.7",
    "type-fest": "^3.11.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/types": "^26.6.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^3.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^26.0.20",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-modal": "^3.13.1",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "rollup": "^2.79.1",
    "rollup-plugin-preserve-directives": "^0.1.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "files": [
    "dist/",
    "package.json",
    "LICENSE.md"
  ]
}